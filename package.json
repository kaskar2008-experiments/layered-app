{
  "name": "vite-project",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:client": "vue-tsc --noEmit && vite build --outDir dist/client",
    "build:server": "vue-tsc --noEmit && vite build --outDir dist/server --ssr src/entry-server.js",
    "ssr": "node server",
    "serve": "vite preview"
  },
  "dependencies": {
    "ky": "^0.27.0",
    "ky-universal": "^0.9.1",
    "parakeet-mapper": "^2.3.0",
    "vue": "^3.0.5",
    "vue-router": "^4.0.5"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "@vitejs/plugin-vue": "^1.1.5",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/server-renderer": "^3.0.9",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "pug": "^3.0.2",
    "stylus": "^0.54.8",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "vite": "^2.1.3",
    "vite-aliases": "^0.5.1",
    "vue-tsc": "^0.0.8"
  }
}
